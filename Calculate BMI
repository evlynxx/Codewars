# Link: https://www.codewars.com/kata/57a429e253ba3381850000fb

def bmi(weight, height):
    BMI = weight/(height**2)
    if BMI <= 18.5:
        return 'Underweight'
    if BMI <= 25.0:
        return 'Normal'
    if BMI <= 30.0:
        return 'Overweight'
    if BMI > 30.0:
        return 'Obese'
